{
  "name": "movielibrary",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "node --trace-warnings .env.js",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "pushupdate:android:dev": "appcenter codepush release-react -a farisbaros1/Movie-Library -d Development",
    "pushupdate:android:staging": "appcenter codepush release-react -a farisbaros1/Movie-Library -d Staging",
    "pushupdate:android:prod": "appcenter codepush release-react -a farisbaros1/Movie-Library -d Production",
    "pushupdate:ios:dev": "appcenter codepush release-react -a farisbaros1/Movie-Library-1 -d Development",
    "pushupdate:ios:staging": "appcenter codepush release-react -a farisbaros1/Movie-Library-1 -d Staging",
    "pushupdate:ios:prod": "appcenter codepush release-react -a farisbaros1/Movie-Library-1 -d Production",
    "android:bundleRelease": "cd android/ && ./gradlew bundleRelease && cd ..",
    "android:assembleRelease": "cd android/ && ./gradlew assembleRelease && cd ..",
    "android:generateApks": "bundletool build-apks --bundle=android/app/build/outputs/bundle/release/app-release.aab --output=apks/app-release.apks --ks=prufast.keystore --ks-pass=pass:Password09 --ks-key-alias=prufast --key-pass=pass:Password09 --overwrite --mode=universal",
    "android:installApks": "bundletool install-apks --apks=apks/app-release.apks",
    "android:install": "adb install android/app/build/outputs/apk/release/app-release.apk",
    "android:clean:windows": "cd android && .\\gradlew clean",
    "ios:clean": "cd ios && rimraf build",
    "start:clean": "react-native start -- --reset-cache",
    "lint:fix": "eslint *.js **/*.js --fix"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.7",
    "@react-native-community/datetimepicker": "^3.5.2",
    "@react-native-community/netinfo": "^6.0.2",
    "@react-native-masked-view/masked-view": "^0.2.6",
    "@react-navigation/bottom-tabs": "^6.0.5",
    "@react-navigation/material-top-tabs": "^6.0.2",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/native-stack": "^6.1.0",
    "@react-navigation/stack": "^6.0.7",
    "appcenter": "4.2.0",
    "appcenter-analytics": "4.2.0",
    "appcenter-crashes": "4.2.0",
    "axios": "^0.21.3",
    "color": "^4.0.1",
    "moment": "^2.29.1",
    "react": "17.0.2",
    "react-native": "0.65.1",
    "react-native-code-push": "^7.0.2",
    "react-native-document-picker": "^6.0.4",
    "react-native-exception-handler": "^2.10.10",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-progress": "^5.0.0",
    "react-native-safe-area-context": "^3.3.0",
    "react-native-screens": "^3.6.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^12.1.1",
    "react-native-tab-view": "^3.1.1",
    "react-native-vector-icons": "^8.1.0",
    "react-native-webview": "^11.13.0",
    "react-redux": "^7.2.4",
    "redux": "^4.1.1",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/react-native": "^0.64.13",
    "babel-jest": "^26.6.3",
    "dotenv": "^10.0.0",
    "eslint": "7.14.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.0",
    "react-native-codegen": "^0.0.7",
    "react-test-renderer": "17.0.2",
    "xml2js": "^0.4.23"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native|@react-native(-community)|react-native|react-native-vector-icons)/)"
    ]
  }
}
